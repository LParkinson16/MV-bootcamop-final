<title>All Projects</title>
<h1>All Projects</h1>
<html lang="en">
    <a href="/projects/new">Add project</a>
<section>
    {{#each projects}}
        <a href="/projects/{{this.id}}">
            <article>
                <main>{{this.name}}</main>
                <footer>{{this.description}}</footer>
            </article>
        </a>
    {{/each}}
</section>
</html>
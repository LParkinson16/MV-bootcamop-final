<header>
  <h1>{{project.name}}</h1>
</header>
<div class="app">
  <div class="containers">

<div class="container" id="toDoDiv">
  <h2>To-Do</h2>
  <a href="/tasks/new">
    <button>Add task</button>
  </a>
    {{#each columns.todo}}
      <div class="task" taskId="{{this.id}}">
        <p>{{this.description}}</p>
      </div>
    {{/each}}
</div>

<div class="container">
  <h2>In progress</h2>
    {{#each columns.inProgress}}
      <div class="task" taskId="{{this.id}}">
        <p>{{this.description}}</p>
      </div>
    {{/each}}
</div>

<div class="container">
  <h2>Completed</h2>
      {{#each columns.done}}
      <div class="task" taskId="{{this.id}}">
        <p>{{this.description}}</p>
      </div>
    {{/each}}
</div>


</div>
</div>
<script src="/script.js" defer></script>